%!

/inch {72 mul} def
/unit {5 inch 6 div mul} def
/centershow {
    /text exch def
    gsave
	text stringwidth pop -2 div 0 rmoveto
	text show
    grestore
} def
/tiltshow {
    /text exch def
    gsave
	currentpoint translate
	30 rotate
	text stringwidth pop -2 div 0 moveto
	gsave
	1 setgray
	gsave
	0 -.5 rmoveto
	text show
	grestore
	.5 0 rmoveto
	text show
	grestore
	text show
    grestore
} def

% sets the icon color based on w
/seticoncolor {
    w 0 eq {
	.4 1 .4 setrgbcolor
    } {
	w 1 eq { 
	    1 .5 .5 setrgbcolor
	}{
	    .7 .4 1 setrgbcolor
	} ifelse
    } ifelse    
} def

% draws the current path based on x
/drawiconpath {
    x 0 eq {
	stroke
    } {
	x 1 eq {
	    gsave
	    clip
	    newpath
	    -.5 unit -.25 unit moveto
	    20 {
		0 1.5 unit rlineto
		1 20 div unit -1.5 unit rmoveto
	    } repeat
	    stroke
	    grestore
	    stroke
	} {
	    fill
	} ifelse
    } ifelse
} def

/being {
    .2 unit 0 moveto
    0 0 -.1 unit .3 unit -.2 unit 0 curveto
    0 0 .1 unit -.3 unit .2 unit 0 curveto
} def

/place {
    .2 unit 0 moveto
    .1 unit 0 .1 unit 0 90 arc
    -.1 unit 0 .1 unit 90 270 arc
    .1 unit 0 .1 unit 270 360 arc

} def

/thing {
    .2 unit 0 moveto
    0 .1 unit lineto
    -.2 unit 0 lineto
    0 -.1 unit lineto
    .2 unit 0 lineto
} def

%draws the correct icon based on y
/drawicon {
    y 0 eq {
	being
    } {
	y 1 eq {
	    place
	} {
	    thing
	} ifelse
    } ifelse
} def

%draws the icon the correct number of times based on z
%calls the other variables
/icon {
    gsave
    currentpoint translate
    seticoncolor

    newpath
    0 z 6 div unit translate
    z 1 add {
	drawicon
	0 -1 3 div unit translate
    } repeat
    drawiconpath
    grestore
} def

